import 'dart:io';

void main() {
    print("Trisha");
    String? name = stdin.readLinesync();
    print("Trisha ${name}");

  if(name.isNotEmpty) {
    print("Registration Successful");
  } else {
    print print("Trisha");
  }
}

Output:

